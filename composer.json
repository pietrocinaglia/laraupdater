{
    "name": "rohsyl/laraupdater",
    "description": "LaraUpdater allows your Laravel Application to auto-update itself.",
    "keywords": [
        "self-updater",
        "update-checker",
        "update-installer",
        "laravel",
        "backup-restore"
    ],
    "homepage": "https://github.com/rohsyl/laraupdater",
    "require": {
        "php": ">=7.1.3",
        "ext-zip": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Roh Sylvain",
            "homepage": "http://rohs.ch",
            "role": "Developer; IT Engineer."
        },
        {
            "name": "Pietro Cinaglia",
            "homepage": "http://linkedin.com/in/pietrocinaglia",
            "role": "Developer; Software Engineer."
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "rohsyl\\laraupdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\laraupdater\\": "src"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "rohsyl\\laraupdater\\LaraUpdaterServiceProvider"
            ]
        }
    }
}
